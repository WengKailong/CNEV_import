{
  "compilerOptions": {
    "lib": [
      "ES2020"
    ],
    "module": "ES2020",
    "target": "ES2020",
    "moduleResolution": "Node",
    "outDir": "lib",
    "rootDir": "src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true
  }
}